# Example Terraform Variables File
# Copy this file to terraform.tfvars and customize as needed
# Note: terraform.tfvars is gitignored and should never be committed

# Server Configuration
# Uncomment and modify the variables you want to override

# List of server names to create
# server_names = ["session-node-1", "session-node-2", "session-node-3"]

# Server specifications (defaults shown)
# server_type     = "cx33"
# server_location = "nbg1"
# server_image    = "ubuntu-24.04"

# SSH key path (defaults shown)
# ssh_public_key_path = "../session_node_key.pub"
